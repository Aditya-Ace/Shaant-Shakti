    "terminal.foreground": "#E8F5E8",
    "terminalCursor.background": "#0F1B0F",
    "terminalCursor.foreground": "#00FFFF",
    "terminal.selectionBackground": "#40E0D044",
    "terminal.ansiBlack": "#0A150A",
    "terminal.ansiRed": "#FF6B6B",
    "terminal.ansiGreen": "#32CD32",
    "terminal.ansiYellow": "#FFD700",
    "terminal.ansiBlue": "#4169E1",
    "terminal.ansiMagenta": "#9370DB",
    "terminal.ansiCyan": "#40E0D0",
    "terminal.ansiWhite": "#E8F5E8",
    "terminal.ansiBrightBlack": "#4F6F4F",
    "terminal.ansiBrightRed": "#FF8A8A",
    "terminal.ansiBrightGreen": "#7FE07F",
    "terminal.ansiBrightYellow": "#FFE55C",
    "terminal.ansiBrightBlue": "#6495ED",
    "terminal.ansiBrightMagenta": "#DA70D6",
    "terminal.ansiBrightCyan": "#00FFFF",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
    {
      "name": "Default",
      "scope": ["source", "text"],
      "settings": { "foreground": "#E8F5E8" }
    },
    {
      "name": "Invalid / Illegal",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#FF6B6B", "fontStyle": "bold" }
    },
    {
      "name": "Comments - Whispers of the Forest",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "fontStyle": "italic", "foreground": "#4F6F4F" }
    },
    {
      "name": "Documentation - Ancient Wisdom",
      "scope": ["comment.block.documentation"],
      "settings": { "foreground": "#8FBC8F", "fontStyle": "italic" }
    },
    {
      "name": "Strings - Nature's Voice",
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#7FE07F" }
    },
    {
      "name": "String Templates - Mystical Incantations",
      "scope": ["string.template", "punctuation.definition.template-string"],
      "settings": { "foreground": "#98FB98" }
    },
    {
      "name": "Regex / Escape - Spirit Patterns",
      "scope": ["string.regexp", "constant.character.escape"],
      "settings": { "foreground": "#40E0D0" }
    },
    {
      "name": "Numbers & Booleans - Sacred Geometry",
      "scope": ["constant.numeric", "constant.language"],
      "settings": { "foreground": "#FFD700", "fontStyle": "bold" }
    },
    {
      "name": "Other Constants - Eternal Truths",
      "scope": ["constant.character", "constant.other"],
      "settings": { "foreground": "#FF69B4" }
    },
    {
      "name": "Keywords - Forest Magic",
      "scope": ["keyword.control", "storage.type", "storage.modifier"],
      "settings": { "fontStyle": "italic bold", "foreground": "#9370DB" }
    },
    {
      "name": "Operators - Energy Flow",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#00FFFF", "fontStyle": "bold" }
    },
    {
      "name": "Punctuation - Spirit Boundaries",
      "scope": ["punctuation", "meta.brace", "meta.delimiter"],
      "settings": { "foreground": "#4F6F4F" }
    },
    {
      "name": "Variables - Living Entities",
      "scope": ["variable", "meta.definition.variable"],
      "settings": { "foreground": "#E8F5E8" }
    },
    {
      "name": "Read/Write Variables - Active Spirits",
      "scope": ["variable.other.readwrite"],
      "settings": { "foreground": "#B8D4B8" }
    },
    {
      "name": "Parameters - Spirit Messengers",
      "scope": ["variable.parameter"],
      "settings": { "fontStyle": "italic", "foreground": "#DDA0DD" }
    },
    {
      "name": "Object Properties - Spirit Attributes",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": { "foreground": "#98FB98" }
    },
    {
      "name": "Functions & Methods - Forest Rituals",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#32CD32", "fontStyle": "bold" }
    },
    {
      "name": "Function & Method Calls - Invoking Spirits",
      "scope": ["meta.function-call", "entity.name.function.call"],
      "settings": { "foreground": "#7FE07F" }
    },
    {
      "name": "Classes - Ancient Guardians",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": { "fontStyle": "bold", "foreground": "#40E0D0" }
    },
    {
      "name": "Types & Interfaces - Spirit Contracts",
      "scope": ["entity.name.type", "support.type", "entity.name.interface"],
      "settings": { "foreground": "#87CEEB", "fontStyle": "italic" }
    },
    {
      "name": "Namespaces / Modules - Sacred Groves",
      "scope": [
        "entity.name.namespace",
        "entity.name.module",
        "entity.name.package"
      ],
      "settings": { "foreground": "#9370DB", "fontStyle": "bold" }
    },
    {
      "name": "Component Tags - Mystical Elements",
      "scope": ["support.class.component", "support.type.component"],
      "settings": { "fontStyle": "bold", "foreground": "#FF69B4" }
    },
    {
      "name": "HTML/JSX/TSX Tags - Elemental Forms",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#32CD32", "fontStyle": "bold" }
    },
    {
      "name": "Tag Attributes - Spirit Powers",
      "scope": ["entity.other.attribute-name"],
      "settings": { "fontStyle": "italic", "foreground": "#FFD700" }
    },
    {
      "name": "Decorators/Annotations - Sacred Symbols",
      "scope": [
        "meta.decorator",
        "storage.type.annotation",
        "punctuation.decorator"
      ],
      "settings": { "fontStyle": "italic bold", "foreground": "#FF8C00" }
    },
    {
      "name": "Markdown Headings - Forest Proclamations",
      "scope": [
        "markup.heading",
        "markup.heading.setext",
        "entity.name.section"
      ],
      "settings": { "foreground": "#FFD700", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Emphasis - Spirit Whispers",
      "scope": ["markup.bold", "markup.italic"],
      "settings": { "foreground": "#40E0D0" }
    },
    {
      "name": "Markdown Code - Ancient Scripts",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": { "foreground": "#7FE07F" }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#B8D4B8",
    "property.readonly": "#98FB98",
    "parameter": { "foreground": "#DDA0DD", "italic": true },
    "function": { "foreground": "#32CD32", "bold": true },
    "method": { "foreground": "#7FE07F", "bold": true },
    "class": { "foreground": "#40E0D0", "bold": true },
    "struct": { "foreground": "#87CEEB", "bold": true },
    "enum": { "foreground": "#9370DB", "bold": true },
    "enumMember": { "foreground": "#FFD700", "bold": true },
    "interface": { "foreground": "#87CEEB", "italic": true },
    "namespace": { "foreground": "#9370DB", "bold": true },
    "typeParameter": { "foreground": "#FF69B4", "italic": true },
    "decorator": { "foreground": "#FF8C00", "italic": true, "bold": true },
    "event": { "foreground": "#FFD700", "bold": true },
    "regexp": { "foreground": "#40E0D0", "bold": true },
    "operator": { "foreground": "#00FFFF", "bold": true },
    "keyword": { "foreground": "#9370DB", "italic": true, "bold": true },
    "comment": { "foreground": "#4F6F4F", "italic": true },
    "string": "#7FE07F",
    "number": { "foreground": "#FFD700", "bold": true },
    "boolean": { "foreground": "#FFD700", "bold": true },
    "type": { "foreground": "#87CEEB", "italic": true }
  }
}
