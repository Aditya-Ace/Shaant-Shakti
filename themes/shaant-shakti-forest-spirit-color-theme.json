  "tokenColors": [
    {
      "name": "Default",
      "scope": ["source", "text"],
      "settings": { "foreground": "#E5F0E6" }
    },
    {
      "name": "Invalid / Illegal",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#FF7A6C" }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "fontStyle": "italic", "foreground": "#7D987D" }
    },
    {
      "name": "Documentation Markup",
      "scope": ["comment.block.documentation"],
      "settings": { "foreground": "#A9C7A1" }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#C6E6B8" }
    },
    {
      "name": "String Templates",
      "scope": ["string.template", "punctuation.definition.template-string"],
      "settings": { "foreground": "#C6E6B8" }
    },
    {
      "name": "Regex / Escape",
      "scope": ["string.regexp", "constant.character.escape"],
      "settings": { "foreground": "#7DB3E0" }
    },
    {
      "name": "Numbers & Booleans",
      "scope": ["constant.numeric", "constant.language"],
      "settings": { "foreground": "#FFE08A" }
    },
    {
      "name": "Other Constants",
      "scope": ["constant.character", "constant.other"],
      "settings": { "foreground": "#FFE08A" }
    },
    {
      "name": "Keywords (Control/Decl)",
      "scope": ["keyword.control", "storage.type", "storage.modifier"],
      "settings": { "fontStyle": "italic", "foreground": "#9AD09C" }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#7D987D" }
    },
    {
      "name": "Punctuation / Braces",
      "scope": ["punctuation", "meta.brace", "meta.delimiter"],
      "settings": { "foreground": "#6E8A6F" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "meta.definition.variable"],
      "settings": { "foreground": "#E5F0E6" }
    },
    {
      "name": "Read/Write Variables",
      "scope": ["variable.other.readwrite"],
      "settings": { "foreground": "#E5F0E6" }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": { "fontStyle": "italic", "foreground": "#E5F0E6" }
    },
    {
      "name": "Object Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": { "foreground": "#CFE3CF" }
    },
    {
      "name": "Functions & Methods (Decl)",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#C6E6B8" }
    },
    {
      "name": "Function & Method Calls",
      "scope": ["meta.function-call", "entity.name.function.call"],
      "settings": { "foreground": "#9AD09C" }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": { "fontStyle": "bold", "foreground": "#A9C7A1" }
    },
    {
      "name": "Types & Interfaces",
      "scope": ["entity.name.type", "support.type", "entity.name.interface"],
      "settings": { "foreground": "#A9C7A1" }
    },
    {
      "name": "Namespaces / Modules",
      "scope": [
        "entity.name.namespace",
        "entity.name.module",
        "entity.name.package"
      ],
      "settings": { "foreground": "#A9C7A1" }
    },
    {
      "name": "Component Tags",
      "scope": ["support.class.component", "support.type.component"],
      "settings": { "fontStyle": "bold", "foreground": "#A9C7A1" }
    },
    {
      "name": "HTML/JSX/TSX Tags",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#9AD09C" }
    },
    {
      "name": "Tag Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": { "fontStyle": "italic", "foreground": "#FFE08A" }
    },
    {
      "name": "Decorators/Annotations",
      "scope": [
        "meta.decorator",
        "storage.type.annotation",
        "punctuation.decorator"
      ],
      "settings": { "fontStyle": "italic", "foreground": "#FFE08A" }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "markup.heading.setext",
        "entity.name.section"
      ],
      "settings": { "foreground": "#FFE08A" }
    },
    {
      "name": "Markdown Emphasis",
      "scope": ["markup.bold", "markup.italic"],
      "settings": { "foreground": "#E5F0E6" }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": { "foreground": "#C6E6B8" }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#E5F0E6",
    "property.readonly": "#E5F0E6",
    "parameter": { "foreground": "#E5F0E6", "italic": true },
    "function": "#9AD09C",
    "method": "#9AD09C",
    "class": { "foreground": "#A9C7A1", "bold": true },
    "struct": "#A9C7A1",
    "enum": "#A9C7A1",
    "enumMember": "#FFE08A",
    "interface": "#A9C7A1",
    "namespace": "#A9C7A1",
    "typeParameter": "#A9C7A1",
    "decorator": { "foreground": "#FFE08A", "italic": true },
    "event": "#FFE08A",
    "regexp": "#7DB3E0",
    "operator": "#7D987D",
    "keyword": { "foreground": "#9AD09C", "italic": true },
    "comment": { "foreground": "#7D987D", "italic": true },
    "string": "#C6E6B8",
    "number": "#FFE08A",
    "boolean": "#FFE08A",
    "type": "#A9C7A1"
  }
}
