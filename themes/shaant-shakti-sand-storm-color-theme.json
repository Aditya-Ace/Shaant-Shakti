  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#8D6E63",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.other.debugger"
      ],
      "settings": {
        "foreground": "#D07C5B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.other.symbol",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#A6B07A"
      }
    },
    {
      "name": "Template Literals",
