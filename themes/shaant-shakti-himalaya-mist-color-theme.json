    "terminal.ansiBrightRed": "#FF8FA3",
    "terminal.ansiBrightGreen": "#90EE90",
    "terminal.ansiBrightYellow": "#FFFF99",
    "terminal.ansiBrightBlue": "#87CEEB",
    "terminal.ansiBrightMagenta": "#DDA0DD",
    "terminal.ansiBrightCyan": "#E0FFFF",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
    {
      "name": "Default - Mountain Air",
      "scope": ["source", "text"],
      "settings": { "foreground": "#F0F8FF" }
    },
    {
      "name": "Invalid - Avalanche Warning",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#FF6B9D", "fontStyle": "bold" }
    },
    {
      "name": "Comments - Whispers of the Wind",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "fontStyle": "italic", "foreground": "#6495ED" }
    },
    {
      "name": "Documentation - Sacred Texts",
      "scope": ["comment.block.documentation"],
      "settings": { "foreground": "#B8D4E8", "fontStyle": "italic" }
    },
    {
      "name": "Strings - Mountain Prayers",
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#B0C4DE" }
    },
    {
      "name": "String Templates - Ancient Mantras",
      "scope": ["string.template", "punctuation.definition.template-string"],
      "settings": { "foreground": "#E6E6FA" }
    },
    {
      "name": "Regex - Mountain Patterns",
      "scope": ["string.regexp", "constant.character.escape"],
      "settings": { "foreground": "#87CEEB" }
    },
    {
      "name": "Numbers & Booleans - Sacred Geometry",
      "scope": ["constant.numeric", "constant.language"],
      "settings": { "foreground": "#FFD700" }
    },
    {
      "name": "Other Constants - Mountain Stones",
      "scope": ["constant.character", "constant.other"],
      "settings": { "foreground": "#DDA0DD" }
    },
    {
      "name": "Keywords - Mountain Commands",
      "scope": ["keyword.control", "storage.type", "storage.modifier"],
      "settings": { "fontStyle": "italic", "foreground": "#4A9EFF" }
    },
    {
      "name": "Operators - Mountain Tools",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#87CEEB" }
    },
    {
      "name": "Punctuation - Mountain Paths",
      "scope": ["punctuation", "meta.brace", "meta.delimiter"],
      "settings": { "foreground": "#6495ED" }
    },
    {
      "name": "Variables - Mountain Elements",
      "scope": ["variable", "meta.definition.variable"],
      "settings": { "foreground": "#F0F8FF" }
    },
    {
      "name": "Read/Write Variables - Living Mountain",
      "scope": ["variable.other.readwrite"],
      "settings": { "foreground": "#E8F4FF" }
    },
    {
      "name": "Parameters - Mountain Gifts",
      "scope": ["variable.parameter"],
      "settings": { "fontStyle": "italic", "foreground": "#E0E6FF" }
    },
    {
      "name": "Object Properties - Mountain Treasures",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": { "foreground": "#B8D4E8" }
    },
    {
      "name": "Functions - Mountain Rituals",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#9370DB", "fontStyle": "bold" }
    },
    {
      "name": "Function Calls - Invoking Mountain Spirits",
      "scope": ["meta.function-call", "entity.name.function.call"],
      "settings": { "foreground": "#DDA0DD" }
    },
    {
      "name": "Classes - Mountain Guardians",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": { "fontStyle": "bold", "foreground": "#E0E6FF" }
    },
    {
      "name": "Types - Mountain Essence",
      "scope": ["entity.name.type", "support.type", "entity.name.interface"],
      "settings": { "foreground": "#B0C4DE" }
    },
    {
      "name": "Namespaces - Mountain Realms",
      "scope": [
        "entity.name.namespace",
        "entity.name.module",
        "entity.name.package"
      ],
      "settings": { "foreground": "#87CEEB" }
    },
    {
      "name": "Component Tags - Mountain Temples",
      "scope": ["support.class.component", "support.type.component"],
      "settings": { "fontStyle": "bold", "foreground": "#E0E6FF" }
    },
    {
      "name": "HTML Tags - Mountain Structures",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#4A9EFF" }
    },
    {
      "name": "Tag Attributes - Mountain Decorations",
      "scope": ["entity.other.attribute-name"],
      "settings": { "fontStyle": "italic", "foreground": "#FFD700" }
    },
    {
      "name": "Decorators - Mountain Blessings",
      "scope": [
        "meta.decorator",
        "storage.type.annotation",
        "punctuation.decorator"
      ],
      "settings": { "fontStyle": "italic", "foreground": "#9370DB" }
    },
    {
      "name": "Markdown Headings - Mountain Peaks",
      "scope": [
        "markup.heading",
        "markup.heading.setext",
        "entity.name.section"
      ],
      "settings": { "foreground": "#E0E6FF", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Emphasis - Mountain Winds",
      "scope": ["markup.bold", "markup.italic"],
      "settings": { "foreground": "#DDA0DD" }
    },
    {
      "name": "Markdown Code - Mountain Inscriptions",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": { "foreground": "#87CEEB" }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#E8F4FF",
    "property.readonly": "#B8D4E8",
    "parameter": { "foreground": "#E0E6FF", "italic": true },
    "function": { "foreground": "#9370DB", "bold": true },
    "method": { "foreground": "#DDA0DD", "bold": true },
    "class": { "foreground": "#E0E6FF", "bold": true },
    "struct": "#B0C4DE",
    "enum": "#87CEEB",
    "enumMember": "#FFD700",
    "interface": "#B0C4DE",
    "namespace": "#87CEEB",
    "typeParameter": "#B0C4DE",
    "decorator": { "foreground": "#9370DB", "italic": true },
    "event": "#FFD700",
    "regexp": "#87CEEB",
    "operator": "#87CEEB",
    "keyword": { "foreground": "#4A9EFF", "italic": true },
    "comment": { "foreground": "#6495ED", "italic": true },
    "string": "#B0C4DE",
    "number": "#FFD700",
    "boolean": "#FFD700",
    "type": "#B0C4DE"
  }
}
