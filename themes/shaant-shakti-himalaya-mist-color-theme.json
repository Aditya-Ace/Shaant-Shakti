  "tokenColors": [
    {
      "name": "Default",
      "scope": ["source", "text"],
      "settings": { "foreground": "#E6EEF4" }
    },
    {
      "name": "Invalid / Illegal",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#FF8892" }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "fontStyle": "italic", "foreground": "#6E8597" }
    },
    {
      "name": "Documentation Markup",
      "scope": ["comment.block.documentation"],
      "settings": { "foreground": "#9DB6C9" }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#C3E0FF" }
    },
    {
      "name": "String Templates",
      "scope": ["string.template", "punctuation.definition.template-string"],
      "settings": { "foreground": "#C3E0FF" }
    },
    {
      "name": "Regex / Escape",
      "scope": ["string.regexp", "constant.character.escape"],
      "settings": { "foreground": "#A6DAFF" }
    },
    {
      "name": "Numbers & Booleans",
      "scope": ["constant.numeric", "constant.language"],
      "settings": { "foreground": "#F6D899" }
    },
    {
      "name": "Other Constants",
      "scope": ["constant.character", "constant.other"],
      "settings": { "foreground": "#F6D899" }
    },
    {
      "name": "Keywords (Control/Decl)",
      "scope": ["keyword.control", "storage.type", "storage.modifier"],
      "settings": { "fontStyle": "italic", "foreground": "#7AC7FF" }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#6E8597" }
    },
    {
      "name": "Punctuation / Braces",
      "scope": ["punctuation", "meta.brace", "meta.delimiter"],
      "settings": { "foreground": "#5C6F7F" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "meta.definition.variable"],
      "settings": { "foreground": "#E6EEF4" }
    },
    {
      "name": "Read/Write Variables",
      "scope": ["variable.other.readwrite"],
      "settings": { "foreground": "#E6EEF4" }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": { "fontStyle": "italic", "foreground": "#E6EEF4" }
    },
    {
      "name": "Object Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": { "foreground": "#CFE0EB" }
    },
    {
      "name": "Functions & Methods (Decl)",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#A4F3D7" }
    },
    {
      "name": "Function & Method Calls",
      "scope": ["meta.function-call", "entity.name.function.call"],
      "settings": { "foreground": "#A4F3D7" }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": { "fontStyle": "bold", "foreground": "#BFD3E0" }
    },
    {
      "name": "Types & Interfaces",
      "scope": ["entity.name.type", "support.type", "entity.name.interface"],
      "settings": { "foreground": "#BFD3E0" }
    },
    {
      "name": "Namespaces / Modules",
      "scope": [
        "entity.name.namespace",
        "entity.name.module",
        "entity.name.package"
      ],
      "settings": { "foreground": "#BFD3E0" }
    },
    {
      "name": "Component Tags",
      "scope": ["support.class.component", "support.type.component"],
      "settings": { "fontStyle": "bold", "foreground": "#BFD3E0" }
    },
    {
      "name": "HTML/JSX/TSX Tags",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#7AC7FF" }
    },
    {
      "name": "Tag Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": { "fontStyle": "italic", "foreground": "#F6D899" }
    },
    {
      "name": "Decorators/Annotations",
      "scope": [
        "meta.decorator",
        "storage.type.annotation",
        "punctuation.decorator"
      ],
      "settings": { "fontStyle": "italic", "foreground": "#A4F3D7" }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "markup.heading.setext",
        "entity.name.section"
      ],
      "settings": { "foreground": "#A4F3D7" }
    },
    {
      "name": "Markdown Emphasis",
      "scope": ["markup.bold", "markup.italic"],
      "settings": { "foreground": "#E6EEF4" }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": { "foreground": "#A4F3D7" }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#E6EEF4",
    "property.readonly": "#E6EEF4",
    "parameter": { "foreground": "#E6EEF4", "italic": true },
    "function": "#A4F3D7",
    "method": "#A4F3D7",
    "class": { "foreground": "#BFD3E0", "bold": true },
    "struct": "#BFD3E0",
    "enum": "#BFD3E0",
    "enumMember": "#F6D899",
    "interface": "#BFD3E0",
    "namespace": "#BFD3E0",
    "typeParameter": "#BFD3E0",
    "decorator": { "foreground": "#A4F3D7", "italic": true },
    "event": "#F6D899",
    "regexp": "#A6DAFF",
    "operator": "#6E8597",
    "keyword": { "foreground": "#7AC7FF", "italic": true },
    "comment": { "foreground": "#6E8597", "italic": true },
    "string": "#C3E0FF",
    "number": "#F6D899",
    "boolean": "#F6D899",
    "type": "#BFD3E0"
  }
}
