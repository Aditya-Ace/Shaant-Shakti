  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8D7569",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#C78C99",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#E2BB77"
      }
    },
    {
      "scope": ["variable", "identifier"],
      "settings": {
        "foreground": "#F3E8DC"
      }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#A6B07A"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#D07C5B"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#D07C5B"
      }
    },
    {
      "scope": ["meta.tag", "entity.name.tag"],
      "settings": {
        "foreground": "#C78C99"
      }
    },
    {
      "scope": ["markup.heading.markdown"],
      "settings": {
        "foreground": "#E2BB77",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold.markdown"],
      "settings": {
        "foreground": "#D07C5B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic.markdown"],
      "settings": {
        "foreground": "#C78C99",
        "fontStyle": "italic"
      }
    }
  ]
}
