  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#5C6370",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator",
        "keyword.other",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.other.debugger"
      ],
      "settings": {
        "foreground": "#FF6AC1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.other.symbol",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#5AF78E"
      }
    },
    {
      "name": "Template Literals",
      "scope": ["string.template", "punctuation.definition.string.template"],
      "settings": {
        "foreground": "#5AF78E"
      }
    },
    {
      "name": "Template Expressions",
      "scope": [
        "meta.template.expression",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#F3F99D"
      }
    },
    {
      "name": "Built-in constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": {
        "foreground": "#57C7FF"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "identifier",
        "meta.definition.variable",
        "meta.definition.variable.name",
        "support.variable"
      ],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#9AEDFE",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter"
      ],
      "settings": {
        "foreground": "#57C7FF"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#57C7FF"
      }
    },
    {
      "name": "CSS Classes and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
