{
  "name": "Shaant Shakti",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#0B0C10",
    "editor.foreground": "#F8F8F2",
    "editorLineNumber.foreground": "#5C6370",
    "editorCursor.foreground": "#C678DD",
    "editor.lineHighlightBackground": "#1c1f26",
    "editor.selectionBackground": "#3E4451",
    "editor.inactiveSelectionBackground": "#2c313a80",
    "editorIndentGuide.background1": "#2c313a",
    "editorIndentGuide.activeBackground1": "#5C6370",
    "sideBar.background": "#0B0C10",
    "sideBar.foreground": "#ABB2BF",
    "sideBarSectionHeader.background": "#0B0C10",
    "sideBarSectionHeader.foreground": "#61AFEF",
    "activityBar.background": "#0B0C10",
    "activityBar.foreground": "#C678DD",
    "activityBar.inactiveForeground": "#5C6370",
    "activityBarBadge.background": "#C678DD",
    "activityBarBadge.foreground": "#0B0C10",
    "titleBar.activeBackground": "#0B0C10",
    "titleBar.activeForeground": "#F8F8F2",
    "titleBar.inactiveBackground": "#0B0C10AA",
    "titleBar.inactiveForeground": "#5C6370",
    "statusBar.background": "#0B0C10",
    "statusBar.foreground": "#61AFEF",
    "tab.activeBackground": "#0B0C10",
    "tab.activeForeground": "#F8F8F2",
    "tab.inactiveBackground": "#0B0C10AA",
    "tab.inactiveForeground": "#5C6370",
    "editorGroupHeader.tabsBackground": "#0B0C10",
    "list.hoverBackground": "#1c1f26",
    "list.activeSelectionBackground": "#2c313a",
    "list.activeSelectionForeground": "#F8F8F2",
    "list.inactiveSelectionBackground": "#1c1f26",
    "list.inactiveSelectionForeground": "#ABB2BF",
    "list.highlightForeground": "#C678DD",
    "scrollbarSlider.background": "#5C637050",
    "scrollbarSlider.hoverBackground": "#5C637080",
    "scrollbarSlider.activeBackground": "#5C6370",
    "panel.background": "#0B0C10",
    "panel.border": "#1c1f26",
    "terminal.background": "#0B0C10",
    "terminal.foreground": "#F8F8F2",
    "terminal.ansiBlack": "#282C34",
    "terminal.ansiRed": "#E06C75",
    "terminal.ansiGreen": "#98C379",
    "terminal.ansiYellow": "#E5C07B",
    "terminal.ansiBlue": "#61AFEF",
    "terminal.ansiMagenta": "#C678DD",
    "terminal.ansiCyan": "#56B6C2",
    "terminal.ansiWhite": "#ABB2BF",
    "terminal.ansiBrightBlack": "#5C6370",
    "terminal.ansiBrightRed": "#E06C75",
    "terminal.ansiBrightGreen": "#98C379",
    "terminal.ansiBrightYellow": "#E5C07B",
    "terminal.ansiBrightBlue": "#61AFEF",
    "terminal.ansiBrightMagenta": "#C678DD",
    "terminal.ansiBrightCyan": "#56B6C2",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "list.dropBackground": "#2c313a",
    "tree.indentGuidesStroke": "#3E4451",
    "list.focusBackground": "#21252B",
    "gitDecoration.addedResourceForeground": "#98C379",
    "gitDecoration.modifiedResourceForeground": "#E5C07B",
    "gitDecoration.deletedResourceForeground": "#E06C75",
    "gitDecoration.untrackedResourceForeground": "#56B6C2",
    "editor.selectionHighlightBackground": "#3E445150",
    "editor.wordHighlightBackground": "#3E445130",
    "editor.wordHighlightStrongBackground": "#3E445160",
    "editor.lineHighlightBorder": "#3E445150",
    "editorBracketMatch.border": "#C678DD",
    "list.hoverForeground": "#FFFFFF",
    "tab.hoverBackground": "#1c1f26",
    "activityBar.activeBorder": "#C678DD",
    "sideBarSectionHeader.border": "#1c1f26",
    "editorGroup.border": "#1c1f2633"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5C6370",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#C678DD",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "scope": ["variable", "identifier", "meta.definition.variable"],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "scope": ["meta.structure.dictionary.json string.quoted.double.json"],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "scope": ["string.quoted.double.json"],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "scope": [
        "punctuation.separator.key-value",
        "punctuation.separator.dictionary.key-value"
      ],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "scope": ["variable.language.this", "variable.language.super"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E5C07B"
      }
    },
    {
      "scope": ["meta.structure.dictionary.json string.quoted.double.json"],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "scope": ["string.quoted.double.json"],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "scope": ["constant.language.json"],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "scope": ["constant.numeric.json"],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "scope": ["punctuation.separator.key-value.json"],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "scope": ["punctuation.separator.dictionary.key-value.json"],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "scope": ["comment.line.double-slash.json"],
      "settings": {
        "foreground": "#5C6370",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.class.component", "entity.name.type.class"],
      "settings": { "foreground": "#E06C75", "fontStyle": "italic" }
    },
    {
      "scope": ["meta.tag", "entity.name.tag"],
      "settings": { "foreground": "#E06C75" }
    },
    {
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#D19A66", "fontStyle": "italic" }
    },
    {
      "scope": ["constant.language"],
      "settings": { "foreground": "#56B6C2" }
    },
    {
      "scope": ["entity.name.type"],
      "settings": { "foreground": "#56B6C2" }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#D19A66",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.heading.markdown"],
      "settings": { "foreground": "#61AFEF", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.bold.markdown"],
      "settings": { "foreground": "#D19A66", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.italic.markdown"],
      "settings": { "foreground": "#C678DD", "fontStyle": "italic" }
    },
    {
      "scope": ["storage", "storage.modifier"],
      "settings": {
        "foreground": "#C678DD",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["entity.name.function.constructor"],
      "settings": {
        "foreground": "#E5C07B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.variable", "support.constant"],
      "settings": {
        "foreground": "#56B6C2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#D19A66",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "meta.embedded, source.groovy.embedded, string.template",
      "settings": {
        "foreground": "#98C379"
      }
    }
  ]
}
