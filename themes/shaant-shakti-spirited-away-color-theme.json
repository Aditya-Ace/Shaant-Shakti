    "list.focusBackground": "#268ac3B0",
    "list.highlightForeground": "#268ac3",
    "list.hoverBackground": "#268ac380",
    "list.inactiveSelectionBackground": "#CFCFCF99",
    "panel.background": "#151515",
    "panel.border": "#383838",
    "panelTitle.activeBorder": "#383838",
